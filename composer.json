{
    "name": "digbang/files",
    "description": "File handling library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Cruz Falcon",
            "email": "jfalcon@digbang.com"
        },
        {
            "name": "Dar√≠o Govergun",
            "email": "dgovergun@digbang.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0.2",
        "digbang/doctrine-extensions": "^3.1",
        "ramsey/uuid": "^4.2",
        "laravel-doctrine/orm": "^1.7 || ^1.8",
        "laravel-doctrine/fluent": "^1.2",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^8.5 || ^9.5"
    },
    "autoload": {
        "psr-4": {
            "Digbang\\Files\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digbang\\Files\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "./vendor/bin/phpunit",
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ]
    }
}
